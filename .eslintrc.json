{
  "extends": ["next/core-web-vitals", "prettier", "plugin:prettier/recommended"],
  "rules": {
    "prettier/prettier": [
      "warn",
      {
        "trailingComma": "es5",
        "bracketSpacing": true,
        "printWidth": 80,
        "tabWidth": 2,
        "singleQuote": true,
        "semi": true,
        "arrowParens": "always",
        "bracketSameLine": true
      }
    ]
  }
}
